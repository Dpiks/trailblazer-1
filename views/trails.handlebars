  <div class="trails" {{!-- id="parallax2" --}}>
    <ul id="dropdown1" class="dropdown-content">
      <li><a href="#!">{{user.username}}</a></li>
      <li><a href="/auth/facebook">Log In</a></li>
      <li><a href="/signup">Sign Up</a></li>
    </ul>
    <nav class="N/A transparent z-depth-0">
      <div class="nav-wrapper">
       <a href="/" class="brand-logo"><img src="/assets/css/img/logo.png"></a>

        <ul class="right hide-on-med-and-down">
          <!-- Dropdown Trigger -->
          <li><a class="dropdown-button" href="/" data-activates="dropdown1">Account Access<i class="material-icons right">arrow_drop_down</i></a></li>
        </ul>
      </div>
    </nav>


    <div class="row" id="trailsRow">

      <h3 id="trailTitle">Trails</h3>
      <h4></h4>

        {{#each results}}
        <div class="col s4 trailsColumn" id='trailsContainer'> 
          <span class="info"><a target='_blank' href="{{this.url}}"><i class="small material-icons" id="trailsBtn">info_outline</i></a></span>
          <h6>{{this.trail_name}}</h6>
          <p class="description">{{this.description}}</p>
        </div>
        {{/each}}
    </div>
  </div>

     
