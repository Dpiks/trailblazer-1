<div class="container" id="parkPage">
  <ul id="dropdown1" class="dropdown-content">
      <li><a href="#!">{{user.username}}</a></li>
      <li><a href="/auth/facebook">Log In</a></li>
      <li><a href="/signup">Sign Up</a></li>
    </ul>
    <nav class="N/A transparent z-depth-0">
      <div class="nav-wrapper">
        <!-- <a href="#!" class="brand-logo">Logo</a> -->
        <ul class="right hide-on-med-and-down">
          <!-- Dropdown Trigger -->
          <li><a class="dropdown-button" href="/" data-activates="dropdown1">Account Access<i class="material-icons right">arrow_drop_down</i></a></li>
        </ul>
      </div>
    </nav>
  {{!-- <div class="container" id="parkDisplay"> --}}
    <div class="row">
      <h3 id="parkTitle">Parks</h3>

      <!-- Park Results -->
      {{#each park_data}}
      <div class="col s4 parkColumn" id="parkContainer">
        <h6>{{this.park_name}}</h6>
        <p>{{this.description}}</p>
        {{!-- <a class="waves-effect waves-light btn N/A transparent" class="parkSubmitBtn" href="#parallax2">More Info</a> --}}
        
        <form action='/api/{{this.latitude}}/{{this.longitude}}'>
{{!--         <input type="hidden" name="longitude" value="{{this.longitude}}">
        <input type="hidden" name="latitude" value="{{this.latitude}}"> --}}
        <button class="waves-effect waves-light btn N/A transparent" class="parkSubmitBtn" href="#parallax2">Trails</button>
        </form>
      </div>
      {{/each}}
    </div>
